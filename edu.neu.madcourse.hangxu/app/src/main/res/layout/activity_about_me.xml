<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.neu.madcourse.hangxu.AboutMeActivity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="296dp"
        android:layout_height="22dp"
        android:text="@string/textview_my_email"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="53dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.33"
        app:layout_constraintVertical_bias="0.18"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="266dp"
        android:layout_height="21dp"
        android:text="@string/textview_my_school_year"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.11"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.218"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="227dp"
        android:layout_height="32dp"
        android:text="@string/textview_phone_id"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.87"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="496dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="161dp"
        android:layout_height="23dp"
        android:text="@string/textview_my_name"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.122"
        app:layout_constraintVertical_bias="0.04"
        android:layout_marginLeft="8dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="240dp"
        android:layout_height="317dp"
        app:srcCompat="@drawable/my_profile"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="143dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintVertical_bias="0.53"/>

</android.support.constraint.ConstraintLayout>
